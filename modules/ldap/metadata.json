{
  "name": "datacentred-ldap",
  "version": "0.2.1",
  "source": "git://github.com/datacentred/datacentred-ldap.git",
  "author": "DataCentred Ltd",
  "license": "Apache License, Version 2.0",
  "summary": "Module for managing OpenLDAP",
  "description": "Install and manage OpenLDAP",
  "project_page": "UNKNOWN",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.1.0"
    }
  ],
  "types": [
    {
      "name": "ldap_entry",
      "doc": "Type to manage LDAP entries",
      "properties": [
        {
          "name": "ensure",
          "doc": "The basic property that the resource should be in.  Valid values are `present`, `absent`."
        }
      ],
      "parameters": [
        {
          "name": "name",
          "doc": "Name of LDAP entry i.e. cn=Foo,ou=Bar,dc=baz,dc=co,dc=uk"
        },
        {
          "name": "host",
          "doc": "Host Address (FQDN or IP) of the LDAP server"
        },
        {
          "name": "base",
          "doc": "LDAP tree base i.e. dc=foo,dc=co,dc=uk"
        },
        {
          "name": "port",
          "doc": "Port of the LDAP server (default 389)"
        },
        {
          "name": "username",
          "doc": "Username of admin account on LDAP server"
        },
        {
          "name": "password",
          "doc": "Password of admin account on LDAP server"
        },
        {
          "name": "attributes",
          "doc": "LDAP entry attributes as a hash i.e. { :givenName => \"Foo\",\n                                                 :objectClass =>\n                                                   [\"top\", \"person\", \"inetorgPerson\"]}"
        },
        {
          "name": "self_signed",
          "doc": "Whether the LDAP server certificate is self-signed"
        }
      ],
      "providers": [
        {
          "name": "ldap",
          "doc": ""
        }
      ]
    }
  ],
